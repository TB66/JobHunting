<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- 命令：
    -Dmybatis.generator.overwrite=true mybatis-generator:generate
    -->
    <context id="SQLiteTables" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 不生成注释 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1/parttimesys"
                        userId="root" password="1qaz@WSX">
        </jdbcConnection>

        <!-- 实体类包名 -->
        <javaModelGenerator targetPackage="com.tanben.jobhunting.bean"
                            targetProject="src/main/java">
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- Dao类包名 -->
        <sqlMapGenerator targetPackage="com.tanben.jobhunting.dao"
                         targetProject="src/main/resources">
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.tanben.jobhunting.dao" targetProject="src/main/java">
        </javaClientGenerator>

        <table tableName="user" enableSelectByExample="false"
               enableDeleteByExample="false" enableCountByExample="false"
               selectByExampleQueryId="true" enableUpdateByExample="false">
        <property name="useActualColumnNames" value="true"/>
        <!-- <generatedKey column="ID" sqlStatement="oracle" identity="true" /> -->
        </table>
        <table tableName="category" enableSelectByExample="false"
               enableDeleteByExample="false" enableCountByExample="false"
               selectByExampleQueryId="true" enableUpdateByExample="false">
